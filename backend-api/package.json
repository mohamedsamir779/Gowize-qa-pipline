    "i18next-fs-backend": "^1.1.5",
    "i18next-http-middleware": "^3.2.1",
    "into-stream": "^6.0.0",
    "ioredis": "^5.0.6",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^5.9.28",
    "mongoose-aggregate-paginate-v2": "^1.0.6",
    "mongoose-paginate-v2": "^1.3.11",
    "mongoose-sequence": "^5.3.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-google-storage": "^1.3.0",
    "mysql2": "^2.3.3",
    "node-gyp": "^6.0.1",
