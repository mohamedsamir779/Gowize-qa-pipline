{
  "name": "modular-backend",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "src"
  ],
  "scripts": {
    "orgstart": "node ./bin/www",
    "start": "set DEBUG=backend:* node --max-old-space-size=512 ./app",
    "dev": "set DEBUG=modular-backend:* && cross-env NODE_ENV=dev nodemon --max-old-space-size=512 --ignore ./locales/ ./app.js",
    "local": "set DEBUG=modular-backend:* && cross-env NODE_ENV=dev node --trace-warnings --max-old-space-size=512 ./app.js",
    "local:dev": "set DEBUG=modular-backend:* && cross-env NODE_ENV=dev NODE_PATH=. node --trace-warnings --max-old-space-size=512 ./app.js",
    "serve": "node bin/www",
    "gen_vapid_keys": "web-push generate-vapid-keys [--json]",
    "build": "cross-env NODE_ENV=dev node ./src/seed/prerun",
    "build:dict": "cross-env NODE_ENV=dev node ./src/seed/dictionary",
    "build:notification": "cross-env NODE_ENV=dev node ./src/seed/notification",
    "seed": "cross-env NODE_ENV=dev node ./src/seed",
    "fixProfit": "cross-env NODE_ENV=dev node ./src/seed/updateProfits",
    "test": "cross-env NODE_ENV=test mocha --recursive src/test",
    "lint": "npx eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "npx eslint \"src/**/*.{js,jsx}\" --fix"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.14.0",
    "@binance/connector": "^1.8.1",
    "@dashevo/dashcore-lib": "^0.19.44",
    "@sendgrid/mail": "^7.6.2",
    "agenda": "^4.3.0",
    "amqplib": "^0.10.3",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.2",
    "bitcoinjs-lib": "^6.0.2",
    "body-parser": "^1.19.2",
    "bunyan": "^1.8.12",
    "candlestick-convert": "^5.1.6",
    "ccxt": "^1.77.79",
    "coininfo": "^5.2.1",
    "coinkey": "^3.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cryptoapis": "github:Crypto-APIs/Crypto_APIs_2.0_SDK_Node.js",
    "debug": "~2.6.9",
    "dotenv": "^8.6.0",
    "ecpair": "^2.1.0",
    "ejs": "^3.1.8",
    "ethereumjs-wallet": "^1.0.2",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "fs-readdir-recursive": "^1.1.0",
    "handlebars": "^4.7.7",
    "http-errors": "^2.0.0",
    "i18n": "^0.15.0",
