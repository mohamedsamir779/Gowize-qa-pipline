    "speakeasy": "^2.0.0",
    "src": "^1.1.2",
    "swagger-ui-express": "^4.3.0",
    "tiny-secp256k1": "^2.2.1",
    "tronweb": "^4.4.0",
    "web-push": "^3.5.0",
    "web3": "^1.8.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "ws": "^8.9.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^4.1.0",
    "mocha": "^9.2.2",
