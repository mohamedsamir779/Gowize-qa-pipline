{
  "name": "@sumsub/websdk-react",
  "version": "2.3.16",
  "description": "SumSub WebSdk React",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "types/src"
  ],
  "typings": "types/src",
  "devDependencies": {
    "@rollup/plugin-typescript": "8.2.5",
    "@testing-library/react": "12.0.0",
    "@types/jest": "27.0.1",
    "@types/react": "16.14.13",
    "@types/react-dom": "16.9.14",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-todo-plz": "1.3.0",
    "express": "4.17.1",
    "jest": "27.0.6",
    "jest-environment-jsdom": "27.0.6",
    "jest-junit": "12.2.0",
    "lint-staged": "11.2.6",
    "node-fetch": "2.6.1",
    "prettier": "3.2.5",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "rollup": "2.55.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "sb": "6.3.7",
    "ts-jest": "27.0.5",
    "tslib": "2.3.0",
    "typescript": "4.9.3",
    "@sumsub/websdk": "2.3.16"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "author": "SumSub",
  "license": "MIT",
  "keywords": [
    "sumsub",
    "web",
    "websdk",
    "react"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "pnpm run build:bundle && pnpm run build:types",
    "build:bundle": "rollup -c",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "test": "pnpm run test:unit",
    "test:unit": "jest",
    "lint": "eslint ./src/* --report-unused-disable-directives",
    "clear": "jest --clearCache",
    "demo": "node -r dotenv/config ./.demo/server.js dotenv_config_path=./.demo/.env",
    "precommit": "lint-staged -q",
    "prepush": "pnpm run test:unit",
    "format": "prettier . --write"
  }
}